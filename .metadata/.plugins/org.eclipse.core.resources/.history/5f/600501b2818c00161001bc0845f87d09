
public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
          
		
		
		int  no=6, numofFactors=0;
		
		for(int j=2; j<=100; j++){
			
			for(int i=1; i<=j; i++){
				double result = (double)j/i;
				
				
				if(result==Math.ceil(result)){
					numofFactors++;
				}
			}
				if(numofFactors==2){
					System.out.println("Prime Number");
				}
				numofFactors=0;
				
		}
		
		
		
	}

}
